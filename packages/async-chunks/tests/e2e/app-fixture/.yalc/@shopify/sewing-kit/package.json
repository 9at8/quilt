{
  "name": "@shopify/sewing-kit",
  "version": "0.59.0-17dafe4b",
  "main": "index.js",
  "types": "./lib/src/index.d.js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "sk": "./bin/sewing-kit",
    "skd": "./bin/sewing-kit-debug",
    "ski": "./bin/sewing-kit-inspect",
    "sewing-kit": "./bin/sewing-kit",
    "sewing-kit-debug": "./bin/sewing-kit-debug",
    "sewing-kit-enterprise": "./bin/sewing-kit-enterprise",
    "sewing-kit-inspect": "./bin/sewing-kit-inspect"
  },
  "scripts": {
    "format": "yarn run lint:js --fix && yarn run lint:ts --fix && prettier '**/*.md' --write",
    "lint:js": "eslint . --max-warnings 0 --format codeframe",
    "lint:ts": "eslint . --ext .ts --max-warnings 0 --format codeframe",
    "lint:prettier": "prettier '**/*.md' --list-different",
    "lint": "yarn run lint:js && yarn run lint:ts && yarn run lint:prettier  --list-different",
    "ts": "tsc --noEmit",
    "pretest:e2e": "yarn run build",
    "pretest:e2e:debug": "yarn run build",
    "test:e2e": "jest --roots 'tests/e2e' --",
    "test:e2e:debug": "node --inspect-brk node_modules/.bin/jest --roots 'tests/e2e' --debug --",
    "test:unit": "jest --roots 'src' 'packages' --",
    "test:unit:watch": "jest --roots 'src' 'packages' --watch",
    "check": "yarn run ts && yarn run test:fail-on-only-in-test && yarn run test:unit && yarn run test:e2e",
    "prebuild": "rm -rf lib",
    "build": "tsc",
    "tophat": "node ./tophat/index.js tophat",
    "postbuild": "node ./scripts/rewrite-typescript-paths-to-relative-requires.js",
    "prepublish": "in-publish && yarn run build && echo 'N/A' > README.md || :"
  },
  "files": [
    "lib",
    "bin",
    "jest",
    "typescript",
    "!*.test.js",
    "!lib/tests"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": {
          "plugins": [
            "babel-plugin-system-import-transformer"
          ],
          "sourceMaps": "inline"
        }
      }
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      ".ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "packages",
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:shopify/typescript",
      "plugin:shopify/typescript-prettier",
      "plugin:shopify/jest"
    ],
    "rules": {
      "no-undefined": "off",
      "no-process-env": "off",
      "require-await": "off",
      "shopify/jest/no-vague-titles": "off",
      "typescript/no-var-requires": "off",
      "jest/consistent-test-it": [
        "error",
        {
          "fn": "it"
        }
      ]
    }
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/graphql": "^0.13.3",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/js-yaml": "^3.11.2",
    "@types/jsdom": "^11.0.0",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.5.2",
    "@types/prettier": "^1.13.1",
    "@types/puppeteer": "^1.5.1",
    "@types/rollup": "0.51.4",
    "@types/semver": "^5.5.0",
    "@types/source-map-support": "^0.4.1",
    "@types/tapable": "^1.0.4",
    "@types/temp": "^0.8.32",
    "@types/webpack": "^4.4.13",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/yargs": "^11.1.1",
    "generic-names": "^1.0.2",
    "in-publish": "^2.0.0",
    "jsdom": "^11.0.0",
    "puppeteer": "^1.6.0",
    "rollup-pluginutils": "^2.0.1",
    "typescript": "^3.0.1",
    "webpack-cli": "^3.1.0"
  },
